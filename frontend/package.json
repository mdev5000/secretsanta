{
	"name": "secretsanta-frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"smui-prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:machine": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --output machine",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@material/typography": "^14.0.0",
		"@neoconfetti/svelte": "^1.0.0",
		"@smui-extra/accordion": "^7.0.0-beta.8",
		"@smui-extra/autocomplete": "^7.0.0-beta.8",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/checkbox": "^7.0.0-beta.8",
		"@smui/chips": "^7.0.0-beta.8",
		"@smui/data-table": "^7.0.0-beta.8",
		"@smui/form-field": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@smui/tab": "^7.0.0-beta.8",
		"@smui/tab-bar": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8",
		"@smui/top-app-bar": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.15.9",
		"@types/cookie": "^0.5.1",
		"@types/js-cookie": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vitest": "^0.30.1"
	},
	"type": "module",
	"dependencies": {
		"@protobuf-ts/runtime": "^2.9.0",
		"axios": "^1.4.0",
		"js-cookie": "^3.0.5",
		"svelte-media": "^0.1.5"
	}
}
